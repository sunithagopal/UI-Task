function onFormSubmit(){
    var formDate=readFormData();

}
function readFormData(){
    var formData={};
    formDate["Employee Name"]=document.getElementById("EmployeeName").value;
    formDate["Employee Id"]=document.getElementById("Employeeid").value;
    formDate["Employee Salary"]=document.getElementById("Employeesalay").value;
    formDate["Employee Address"]=document.getElementById("Employeeaddress").value;
    formDate["Employee Phone no"]=document.getElementById("Employeephoneno").value;
    formDate["Employee Email"]=document.getElementById("Employeeemail").value;
return FormData;
}
